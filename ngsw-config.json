{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name":"books",
      "urls": [
        "https://api.myjson.com/bins/j82l2"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "2m",
        "timeout": "3s",
        "strategy": "performance"
      }
    }
  ]
}
